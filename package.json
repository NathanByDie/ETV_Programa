{
  "name": "react-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite --port=3000 --host=0.0.0.0",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "windows": "react-native-windows",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "@react-native-picker/picker": "^2.11.4",
    "@tailwindcss/vite": "^4.1.14",
    "@types/cors": "^2.8.19",
    "@vitejs/plugin-react": "^4.3.1",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "expo": "^55.0.4",
    "expo-constants": "~17.0.3",
    "expo-linking": "~7.0.3",
    "expo-status-bar": "^55.0.4",
    "express": "^4.21.2",
    "firebase": "^10.12.0",
    "konva": "^9.3.16",
    "lucide-react": "^0.378.0",
    "motion": "^10.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-konva": "^18.2.10",
    "react-native": "^0.84.0",
    "react-native-svg": "^15.15.3",
    "react-native-web": "^0.21.2",
    "react-native-windows": "^0.81.4",
    "react-router-dom": "^6.23.0",
    "tailwind-merge": "^2.3.0",
    "twrnc": "^4.16.0",
    "use-image": "^1.1.4",
    "vite": "^5.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-native": "^0.72.8",
    "autoprefixer": "^10.4.19",
    "babel-preset-expo": "^55.0.10",
    "concurrently": "^8.2.2",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.etv.programa",
    "productName": "Programa ETV",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "files": [
      "dist/**/*",
      "main.cjs"
    ]
  }
}
